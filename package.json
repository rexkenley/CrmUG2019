{
  "name": "crmUG",
  "version": "1.0.0",
  "description": "A sample project for 'Advance Dynamics 365 CE Customization utilizing modern JS' presentation",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.6.2",
    "@types/node": "^12.7.11",
    "@types/powerapps-component-framework": "^1.2.1",
    "@uifabric/icons": "^7.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.2.1",
    "cucumber": "^5.1.0",
    "date-fns": "^2.4.1",
    "libphonenumber-js": "^1.7.25",
    "lodash": "^4.17.15",
    "npm-check": "^5.9.0",
    "office-ui-fabric-react": "^7.39.2",
    "pug": "^2.0.4",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "validator": "^11.1.0",
    "webpack": "^5.0.0",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@types/react": "^16.9.5",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "eslint": "^6.5.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^24.9.0",
    "pcf-scripts": "^1.0.6",
    "pcf-start": "^1.0.6",
    "prettier": "^1.18.2",
    "pug-cli": "^1.0.0-alpha6",
    "webpack-cli": "^3.3.9"
  },
  "scripts": {
    "//NPM": "*****************************************************************",
    "buildDev": "npx eslint src && npx webpack --config webpack/config.dev.js",
    "buildDevChunk": "npx eslint src && npx webpack --config webpack/config.dev.chunk.js",
    "buildDevForm": "npx eslint src && npx webpack --config webpack/config.dev.form.js",
    "buildPrd": "npx webpack --config webpack/config.prd.js",
    "buildPrdChunk": "npx webpack --config webpack/config.prd.chunk.js",
    "buildPrdForm": "npx webpack --config webpack/config.prd.form.js",
    "//Form": "*****************************************************************",
    "build:account:dev": "%npm_package_scripts_buildDevForm% src\\js\\account.form.js -o dist\\account.form.js",
    "build:account:prd": "%npm_package_scripts_buildPrdForm% src\\js\\account.form.js -o dist\\account.form.js",
    "//Chunk": "*****************************************************************",
    "build:crmUG:dev": "%npm_package_scripts_buildDevChunk% src\\js\\crmUG.js -o dist\\crmUG.js && npx pug src\\pug\\crmUG.pug -o dist",
    "build:crmUG:prd": "%npm_package_scripts_buildPrdChunk% src\\js\\crmUG.js -o dist\\crmUG.js && npx pug src\\pug\\crmUG.pug -o dist",
    "//PAC": "*****************************************************************",
    "build": "pcf-scripts build",
    "clean": "pcf-scripts clean",
    "rebuild": "pcf-scripts rebuild",
    "start": "pcf-scripts start"
  },
  "keywords": [],
  "author": "Rex Kenley Tan",
  "license": "ISC"
}
